pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "Build, build, build"
            }
        }
        stage('Test') {
            steps {
                echo "Test"
            }
        }
        stage('Deliver') {
            steps {
                echo "Deliver"
            }
        }
        stage('Complete') {
            steps {
                echo 'Job Complete!'
            }
        }
    }
}
